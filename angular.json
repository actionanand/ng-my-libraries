{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "version": 1,
  "projects": {
    "ar-input": {
      "root": "apps/ar-input",
      "projectType": "application",
      "sourceRoot": "apps/ar-input/src",
      "prefix": "ng-my-libraries",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/apps/ar-input",
            "index": "apps/ar-input/src/index.html",
            "main": "apps/ar-input/src/main.ts",
            "polyfills": "apps/ar-input/src/polyfills.ts",
            "tsConfig": "apps/ar-input/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "apps/ar-input/src/favicon.ico",
              "apps/ar-input/src/assets"
            ],
            "styles": [
              "apps/ar-input/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "apps/ar-input/src/environments/environment.ts",
                  "with": "apps/ar-input/src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production",
          "dependsOn": [
            "^build"
          ]
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "ar-input:build:production"
            },
            "development": {
              "browserTarget": "ar-input:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "ar-input:build"
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "apps/ar-input/**/*.ts",
              "apps/ar-input/**/*.html"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/apps/ar-input"
          ],
          "options": {
            "jestConfig": "apps/ar-input/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "deploy": {
          "builder": "angular-cli-ghpages:deploy"
        }
      },
      "tags": []
    },
    "ar-input-e2e": {
      "root": "apps/ar-input-e2e",
      "$schema": "../../node_modules/nx/schemas/project-schema.json",
      "sourceRoot": "apps/ar-input-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/ar-input-e2e/cypress.json",
            "devServerTarget": "ar-input:serve:development"
          },
          "configurations": {
            "production": {
              "devServerTarget": "ar-input:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": [
            "{options.outputFile}"
          ],
          "options": {
            "lintFilePatterns": [
              "apps/ar-input-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": [
        "ar-input"
      ]
    },
    "fa-input": {
      "root": "libs/fa-input",
      "projectType": "library",
      "sourceRoot": "libs/fa-input/src",
      "prefix": "ng-my-libraries",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/fa-input"
          ],
          "options": {
            "project": "libs/fa-input/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/fa-input/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/fa-input/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production",
          "dependsOn": [
            "^build"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/fa-input"
          ],
          "options": {
            "jestConfig": "libs/fa-input/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/fa-input/**/*.ts",
              "libs/fa-input/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "mask": {
      "root": "libs/mask",
      "projectType": "library",
      "sourceRoot": "libs/mask/src",
      "prefix": "ng-my-libraries",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/mask"
          ],
          "options": {
            "project": "libs/mask/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/mask/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/mask/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production",
          "dependsOn": [
            "^build"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/mask"
          ],
          "options": {
            "jestConfig": "libs/mask/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/mask/**/*.ts",
              "libs/mask/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "modal": {
      "root": "libs/modal",
      "projectType": "library",
      "sourceRoot": "libs/modal/src",
      "prefix": "ng-my-libraries",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/modal"
          ],
          "options": {
            "project": "libs/modal/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/modal/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/modal/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production",
          "dependsOn": [
            "^build"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/modal"
          ],
          "options": {
            "jestConfig": "libs/modal/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/modal/**/*.ts",
              "libs/modal/**/*.html"
            ]
          }
        }
      },
      "tags": []
    },
    "tab": {
      "root": "libs/tab",
      "projectType": "library",
      "sourceRoot": "libs/tab/src",
      "prefix": "ng-my-libraries",
      "architect": {
        "build": {
          "builder": "@nrwl/angular:package",
          "outputs": [
            "dist/libs/tab"
          ],
          "options": {
            "project": "libs/tab/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "libs/tab/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "libs/tab/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production",
          "dependsOn": [
            "^build"
          ]
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/tab"
          ],
          "options": {
            "jestConfig": "libs/tab/jest.config.ts",
            "passWithNoTests": true
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/tab/**/*.ts",
              "libs/tab/**/*.html"
            ]
          }
        }
      },
      "tags": []
    }
  },
  "npmScope": "ng-my-libraries",
  "affected": {
    "defaultBase": "main"
  },
  "cli": {
    "packageManager": "npm",
    "analytics": "ce735a79-c6df-4f83-9d9f-6efd63f8afe9"
  },
  "implicitDependencies": {
    "package.json": {
      "dependencies": "*",
      "devDependencies": "*"
    },
    ".eslintrc.json": "*"
  },
  "tasksRunnerOptions": {
    "default": {
      "runner": "nx/tasks-runners/default",
      "options": {
        "cacheableOperations": [
          "build",
          "lint",
          "test",
          "e2e"
        ]
      }
    }
  },
  "targetDefaults": {
    "build": {
      "dependsOn": [
        "^build"
      ]
    }
  },
  "schematics": {
    "@nrwl/angular:application": {
      "style": "scss",
      "linter": "eslint",
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "linter": "eslint",
      "unitTestRunner": "jest"
    },
    "@nrwl/angular:component": {
      "style": "scss"
    }
  },
  "defaultProject": "ar-input"
}